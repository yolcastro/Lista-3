//Sem strcmp

#include <stdio.h>
#include <string.h>

#define TAM 100

void comparar(char* a, char* b) {
    int diferentes = 0;

    while (*a != '\0' && *b != '\0') {
        if (*a == *b) {
            a++;
            b++;
        }
        else {
            diferentes = 1;
            puts("As strings são diferentes.");
            break;
        }
    }

    if (diferentes == 0 && *a == *b) {
        puts("As strings são iguais.");
    }
    else if (diferentes == 0) {
        puts("As strings são diferentes.");
    }
}

int main() {
    char str1[TAM], str2[TAM];

    puts("String 1:");
    fgets(str1, TAM, stdin);
    puts("String 2:");
    fgets(str2, TAM, stdin);

    comparar(str1, str2);
    
    return 0;
}
