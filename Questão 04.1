//Com strcmp

#include <stdio.h>
#include <string.h>

#define TAM 100

int main() {
    char str1[TAM], str2[TAM];

    puts("String 1:");
    fgets(str1, TAM, stdin);
    puts("String 2:");
    fgets(str2, TAM, stdin);

    str1[strcspn(str1, "\n")] = '\0';
    str2[strcspn(str2, "\n")] = '\0';

    int iguais = strcmp(str1, str2);

    if (iguais == 0) {
        puts("As strings são iguais.");
    } else {
        puts("As strings são diferentes.");
    }

    return 0;
}

