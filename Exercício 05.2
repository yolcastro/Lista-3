//Sem strcat

#include <stdio.h>
#include <string.h>

#define TAM 100

void concatenar(char str1[], char str2[]) {
    char *var = str1 + strlen(str1);  
    while ((*var++ = *str2++));     
}

int main() {
    char str1[TAM], str2[TAM];

    printf("String 1:\n");
    fgets(str1, TAM, stdin);
    str1[strcspn(str1, "\n")] = '\0';  

    printf("String 2:\n");
    fgets(str2, TAM, stdin);
    str2[strcspn(str2, "\n")] = '\0';  

    concatenar(str1, str2); 

    printf("Resultado: %s\n", str1);

    return 0;
}
